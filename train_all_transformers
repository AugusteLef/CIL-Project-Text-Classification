bsub -W 24:00 -R "rusage[mem=8192]" -R "rusage[ngpus_excl_p=1]" -R "select[gpu_model0==GeForceGTX1080Ti]" -oo Output_1/bart_raw.out python3 training_huggingface.py -v -c facebook/bart-base -e 3 -bs 32 -as 16 Data/train_neg_full.txt Data/train_pos_full.txt $SCRATCH/Models_1/bart_raw
